{% extends 'base_form.html.twig' %}

{% block body2 %}
<div class="container bs-docs-container">
    <div class="panel panel-default", style="width:100%">
        <table class="table" width="100%" cellspacing="0">
            <thead>
                <tr>
                    <th>{{ 'Id'|trans }}</th>
                    <th>{{ 'valid'|trans }}</th>
                    <th>{{ 'error'|trans }}</th>
                    <th>{{ 'description'|trans }}</th>
                </tr>
            </thead>
            <tfoot>
            </tfoot>
            <tbody>
                {% for apikeyuse in apikeyuses %}
                <tr>
                    <td><a href="{{ path('admin_apikey_use', {'id': apikeyuse.id}) }}">{{apikeyuse.datetime | date("Y-m-d-H:i")}}</td>
                    <td>{{apikeyuse.valid}}</td>
                    <td>{{apikeyuse.my_error}}</td>
                    <td>{{apikeyuse.url}}</td>
                </tr>
                {% endfor %}
            </tbody>
        </table>
    </div>
</div>
{% endblock %}
